{
    "name": "opendesa/opendk",
    "description": "OpenDK adalah aplikasi yang bisa digunakan oleh Pemerintah Kecamatan di Seluruh Indonesia",
    "license": "GPL-3.0-or-later",
    "type": "project",
    "keywords": [
        "OpenDK",
        "Dashboard Kecamatan"
    ],
    "homepage": "https://opendk.my.id/",
    "require": {
        "php": ">=7.4.0",
        "bensampo/laravel-enum": "^2.2|^4.0",
        "cviebrock/eloquent-sluggable": "^7.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "guzzlehttp/psr7": "^2.4",
        "jaybizzle/crawler-detect": "1.*",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.3",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.2",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.2",
        "orangehill/iseed": "^2.6",
        "proengsoft/laravel-jsvalidation": "^4.4",
        "rachidlaasri/laravel-installer": "^4.1",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "sentry/sentry-laravel": "^2.12",
        "spatie/laravel-collection-macros": "^6.1",
        "spatie/laravel-permission": "^5.5",
        "stevebauman/purify": "^4.0",
        "tymon/jwt-auth": "^1.0",
        "unisharp/laravel-filemanager": "^2.6",
        "willvincent/feeds": "^2.2",
        "yajra/laravel-datatables-oracle": "~9."
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-query-detector": "^1.5",
        "fakerphp/faker": "^1.10",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^3.9",
        "laravel/tinker": "^2.5",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "helpers/general_helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ]
    }
}
